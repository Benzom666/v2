      font-style: normal;
      font-weight: 700;
      font-size: 15px;
      line-height: 18px;
      color: #3A5E77;
      padding: 0.5rem 2rem;
    }

    .answer {
      font-family: 'Roboto';
      font-style: normal;
      font-weight: 400;
      font-size: 11px;
      line-height: 13px;
      color: #3A5E77;
      padding: 0 2rem;
      text-align: start;
    }


    ul {
      list-style: disc;
      list-style-position: inside;
      color: #3A5E77;
      padding-left: 30px;
    }

    li {
      font-family: 'Roboto';
      font-style: normal;
      font-weight: 400;
      font-size: 11px;
      line-height: 13px;
      color: #3A5E77;
    }

    .answer-inside-heading {
      padding: 0 2rem;
    }

    .card-bottom-text,
    p {
      padding: 10px 15px;
      text-align: start;
    }

    @media only screen and (max-width:480px) {
      .main_content_card {
        width: 312px !important;
      }

      .qestion {
        padding: 0 !important;
      }

      .answer {
        padding: 0 !important;
      }

      .answer-inside-heading {
        padding: 0 !important;
      }

      ul {
        padding-left: 0 !important;
      }

      .card-bottom-text,
      p {
        padding: 8px;
        text-align: center !important;
      }

      .column_1 {
        width: auto;
        max-width: initial;
      }

      .footer-icons,
      .footer-text-1,
      .footer-text-2 {
        text-align: center;
      }
    }
  </style>
</head>

