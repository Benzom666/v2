      border-spacing: 0;
      font-family: 'Roboto';
      color: #fff;
    }

    .column {
      margin: 2rem auto;
    }

    .two-column {
      text-align: center;
    }

    .two-column .column {
      width: 100%;
      max-width: 300px;
      display: inline-block;
      vertical-align: top;
      text-align: center;
    }

    .main-content-btn-section {
      text-align: center;
    }

    .footer-icons {
      text-align: start;
    }

    .column_1 {
      width: 100%;
      max-width: 300px;
      display: inline-block;
      vertical-align: top;
      text-align: center;
      margin: 5px auto;
    }

    .footer-text-2 {
      text-align: start;
    }

    .main_content_card {
      width: 556px;
      background: linear-gradient(168.08deg, #F2F5F7 4.24%, #E6EBEE 94.68%);
      box-shadow: 6px 6px 10px #D3DADE, -6px -6px 10px 1px rgba(255, 255, 255, 0.6);
      border-radius: 47px;
      margin: 2rem auto;
    }

    .card_ques_ans_section {
      margin: 0.5rem 1rem;
    }

    .qestion {
