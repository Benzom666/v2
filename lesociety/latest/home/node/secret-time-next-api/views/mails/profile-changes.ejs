    body {
      margin: 0;
      background-color: white;
    }

    table {
      border-spacing: 0;
    }

    td {
      padding: 0;
    }

    img {
      border: 0;
    }

    .wrapper {
      width: 100%;
      table-layout: fixed;
      background-color: #fff;
      padding-bottom: 60px;
    }

    .main {
      background-color: #F2F5F7;
      margin: 0 auto;
